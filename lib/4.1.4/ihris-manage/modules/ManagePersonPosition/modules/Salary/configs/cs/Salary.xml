<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.1.9.0</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="cs">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_view_child_form_salary" locale="cs"></configuration>
      <configuration name="person_can_edit_child_form_salary" locale="cs"></configuration>
      <configuration name="can_edit_database_list_salary_source" locale="cs">
        <value>Upravit seznam zdroj_výplaty</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="cs">
        <value>Prohlédnout seznam zdroj_výplaty</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="cs">
            <value>Plat</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="cs">
            <value>Zdroj platu</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Počáteční datum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Plat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Koncové datum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Poznámky</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>