<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-Salary" path="/I2CE">
  <metadata>
    <version>4.1.9.0</version>
  </metadata>
  <configurationGroup name="ihris-manage-Salary" path="/I2CE" locale="es">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_can_view_child_form_salary" locale="es">
        <value>Puede ver formulario secundario de salario de una persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_salary" locale="es">
        <value>Puede editar el subformulario de notas de una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_salary_source" locale="es">
        <value>Editar fuente de la lista de _salario</value>
      </configuration>
      <configuration name="can_view_database_list_salary_source" locale="es">
        <value>Mostrar fuente de la lista de _salario</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="salary">
          <configuration name="display" values="single" locale="es">
            <value>Salario</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="salary_source">
          <configuration name="display" values="single" locale="es">
            <value>Fuente de Ingresos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Salary">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de Inicio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="salary">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Salario</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de Termino</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>