<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="SelfService" path="/">
  <metadata>
    <version>4.1.6.0</version>
  </metadata>
  <configurationGroup name="SelfService" path="/" locale="es">
    <version>4.1.0</version>
    <configurationGroup name="SS" path="/modules/SelfService">
      <configurationGroup name="page">
        <configurationGroup name="view">
          <configurationGroup name="args">
            <configuration name="title" locale="es">
              <value>Ver</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="person_can_view_child_form_user_map" locale="es"></configuration>
      <configuration name="person_can_edit_child_form_user_map" locale="es"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="user_map">
          <configuration name="display" values="single" locale="es"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_UserMap">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Usuario</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page" path="/modules/SelfService/page">
      <configurationGroup name="userAccount">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>AÃ±adir/Actualizar Cuenta de Usuario</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>