<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserStatistics" path="/modules/UserStatistics">
  <metadata>
    <version>4.1.9.0</version>
  </metadata>
  <configurationGroup name="UserStatistics" path="/modules/UserStatistics" locale="cs">
    <version>4.1.5.18</version>
    <configuration name="display_name" locale="cs">
      <value>Statistiky uživatele</value>
    </configuration>
    <configuration name="description" locale="cs">
      <value>Seznam všech změn provedených v jednotlivých polí formuláře pro všechny uživatele.</value>
    </configuration>
    <configurationGroup name="fields">
      <configurationGroup name="date">
        <configuration name="header" locale="cs">
          <value>Datum změny</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="greaterthan_equals">
            <configuration name="header" locale="cs">
              <value>Datum změn dne nebo poté</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="username">
        <configuration name="header" locale="cs">
          <value>Jméno uživatele</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="header" locale="cs">
              <value>Jméno uživatele</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="user">
        <configuration name="header" locale="cs">
          <value>Uživatel</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="form">
        <configuration name="header" locale="cs">
          <value>Formulář</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="header" locale="cs">
              <value>Formulář</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="field">
        <configuration name="header" locale="cs">
          <value>Pole</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="record">
        <configuration name="header" locale="cs">
          <value>ID záznamu</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="header" locale="cs">
              <value>ID záznamu</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="value">
        <configuration name="header" locale="cs">
          <value>Hodnota</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="change_type">
        <configuration name="header" locale="cs">
          <value>Změnit typ</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="display" locale="cs">
      <configuration name="change_type" type="delimited">
        <value>1:Úvodní záznam</value>
        <value>2:Ověřená hodnota</value>
        <value>3:Opravená hodnota</value>
        <value>4:Aktualizovaná hodnota</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Statistiky uživatele</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>