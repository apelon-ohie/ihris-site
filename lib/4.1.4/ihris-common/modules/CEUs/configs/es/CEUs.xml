<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CEUs" path="/I2CE">
  <metadata>
    <version>4.1.0</version>
  </metadata>
  <configurationGroup name="CEUs" path="/I2CE" locale="es">
    <version>4.1.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_continuing_education_course" locale="es">
        <value>Modificar la lista de educación permanente</value>
      </configuration>
      <configuration name="can_view_database_list_continuing_education_course" locale="es">
        <value>Ver la lista continua de educación</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="continuing_education">
          <configuration name="display" values="single" locale="es">
            <value>educación permanente</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="continuing_education_course">
          <configuration name="display" values="single" locale="es">
            <value>Continua su Educación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ContinuingEducationCourse">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Nombre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Horas de crédito</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ContinuingEducation">
          <configurationGroup name="fields">
            <configurationGroup name="continuing_education_course">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Continua su Educación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Horas de crédito</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de Inicio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de Finalización</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>