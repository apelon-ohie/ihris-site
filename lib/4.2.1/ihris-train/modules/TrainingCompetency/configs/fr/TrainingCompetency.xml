<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainingCompetency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="TrainingCompetency" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_trainingcompetency" locale="fr">
        <value>Modifier la liste des compétences de formation</value>
      </configuration>
      <configuration name="can_view_database_list_trainingcompetency" locale="fr">
        <value>Afficher la liste des compétences de formation</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="trainingcompetency">
          <configuration name="display" values="single" locale="fr">
            <value>Information sur la compétence de formation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_TrainingCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Compétence</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>