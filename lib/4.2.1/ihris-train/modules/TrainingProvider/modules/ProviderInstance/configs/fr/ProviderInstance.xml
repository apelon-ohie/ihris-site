<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ProviderInstance" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ProviderInstance" path="/I2CE" locale="fr">
    <version>4.2.0.1.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="provider_can_view_child_form_provider_instance" locale="fr">
        <value>Peut voir le formulaire enfant d’instance d’un fournisseur</value>
      </configuration>
      <configuration name="provider_can_edit_child_form_provider_instance" locale="fr">
        <value>Peut modifier les formulaires enfant d’instance d’un fournisseur</value>
      </configuration>
      <configuration name="can_edit_database_list_funder" locale="fr">
        <value>Peut modifier la liste des bases de données des bailleurs de fonds.</value>
      </configuration>
      <configuration name="can_view_database_list_funder" locale="fr">
        <value>Peut voir la liste des bases de données des bailleurs de fonds.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="provider_instance">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Information de formation planifiée/mise à jour</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="fee">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour les informations de frais</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="editexams">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Fixer les détails des examens</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="provider_instance">
          <configuration name="display" values="single" locale="fr">
            <value>Information de formation planifiée</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fee">
          <configuration name="display" values="single" locale="fr">
            <value>Information sur les frais d’une formation planifiée</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="funder">
          <configuration name="display" values="single" locale="fr">
            <value>Bailleur de fonds</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ProviderInstance">
          <configurationGroup name="list_default" path="meta/list/default">
            <configuration name="display_string" locale="fr"></configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="cancelled">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de début</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de fin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="academic_year">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Années académique (par ex. Année 1)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="calender_year">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Année calendaire</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="participants_list">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Télécharger la liste scannée des étudiants ayant signé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="funder">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Source de financement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Groupe(s) cible</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Détails de l’emplacement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="providertraining">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Fournisseur de formation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="i2ce_hidden">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Masquer</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Fee">
          <configurationGroup name="fields">
            <configurationGroup name="fee_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Frais</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Montant</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>