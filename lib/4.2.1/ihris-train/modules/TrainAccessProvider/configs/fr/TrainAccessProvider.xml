<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainAccessProvider">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="forms" path="/modules/forms" locale="fr">
    <version>4.2.0.2.1</version>
    <configurationGroup name="forms">
      <configurationGroup name="access_provider">
        <configuration name="display" values="single" locale="fr">
          <value>Fournisseur d’accès utilisateur</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="formClasses">
      <configurationGroup name="iHRIS_UserAccessProvider">
        <configurationGroup name="fields">
          <configurationGroup name="provider">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Fournisseur</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page" path="/I2CE/page">
      <configurationGroup name="access_provider">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour l’accès au fournisseur</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>