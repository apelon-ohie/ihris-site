<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ProviderTraining" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ProviderTraining" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_providertraining" locale="fr">
        <value>Modifier la liste des fournisseurs de formation</value>
      </configuration>
      <configuration name="can_view_database_list_providertraining" locale="fr">
        <value>Afficher la liste des fournisseurs de formation</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="providertraining">
          <configuration name="display" values="single" locale="fr">
            <value>Information sur un fournisseur de formation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ProviderTraining">
          <configurationGroup name="fields">
            <configurationGroup name="trainingprovider">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Fournisseur</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>