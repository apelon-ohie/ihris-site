<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports-full-search_person" path="/modules/CustomReports">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports-full-search_person" path="/modules/CustomReports" locale="es">
    <version>4.2.0.1</version>
    <configurationGroup name="search_person" path="//modules/CustomReports/relationships/search_person">
      <configuration name="display_name" locale="es">
        <value>Buscar persona</value>
      </configuration>
      <configuration name="description" locale="es">
        <value>Búsqueda de relación de los participantes</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="demographic">
          <configuration name="description" locale="es">
            <value>Muestra la información demográfica de la persona</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Demográfico</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="description" locale="es">
            <value>Muestra la información de posición de la persona</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Posición</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configuration name="description" locale="es">
            <value>Muestra la información de registro de la persona</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Registro</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_person" path="//modules/CustomReports/reports/search_person">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="es">
          <value>Buscar persona</value>
        </configuration>
        <configuration name="description" locale="es">
          <value>Buscar registros de todas las personas</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="firstname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="es">
                    <value>Primer nombre</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Primer nombre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nationality">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Nacionalidad</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Nacionalidad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="header" locale="es">
                <value>Otros nombres</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="surname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="es">
                    <value>Apellido</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Apellido</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="village">
              <configuration name="header" locale="es">
                <value>Pueblo natal</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Consejo de registro</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Consejo de registro</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="header" locale="es">
                <value>Fecha de vencimiento de la licencia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="header" locale="es">
                <value>Número de licencia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="header" locale="es">
                <value>Fecha de registro</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="header" locale="es">
                <value>Número de registro</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="position">
          <configurationGroup name="fields">
            <configurationGroup name="health_facility">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Centro de salud</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Centro de salud</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Título de Trabajo/Puesto</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="in">
                  <configuration name="header" locale="es">
                    <value>Título de Trabajo/Puesto</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Título de Trabajo/Puesto</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="header" locale="es">
                <value>Fecha de nacimiento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Sexo</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Sexo</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_people" path="//modules/CustomReports/reportViews/search_people">
      <configuration name="description" locale="es">
        <value>Buscar todas las personas en el sistema</value>
      </configuration>
      <configuration name="display_name" locale="es">
        <value>Buscar persona</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>