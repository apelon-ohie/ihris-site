    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports-full-search_person" path="/modules/CustomReports" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="search_person" path="//modules/CustomReports/relationships/search_person">
      <configuration name="display_name" locale="fr">
        <value>Rechercher une personne</value>
      </configuration>
      <configuration name="description" locale="fr">
        <value>Rechercher une relation pour les participants</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="demographic">
          <configuration name="description" locale="fr">
            <value>Affiche les informations démographiques de la personne</value>
          </configuration>
          <configuration name="display_name" locale="fr">
            <value>Données démographiques</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="description" locale="fr">
            <value>Affiche les informations démographiques de la personne</value>
          </configuration>
          <configuration name="display_name" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configuration name="description" locale="fr">
            <value>Affiche les informations démographiques de la personne</value>
          </configuration>
          <configuration name="display_name" locale="fr">
            <value>Inscription</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_person" path="//modules/CustomReports/reports/search_person">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="fr">
          <value>Rechercher une personne</value>
        </configuration>
        <configuration name="description" locale="fr">
          <value>Rechercher les enregistrements de tout le monde</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="firstname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="fr">
                    <value>Prénom</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Prénom</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nationality">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Nationalité</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Nationalité</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="header" locale="fr">
                <value>Autres noms</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="surname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="fr">
                    <value>Nom de famille</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Nom de famille</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="village">
              <configuration name="header" locale="fr">
                <value>Village de domicile</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="registraion">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Conseil d’inscription</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Conseil d’inscription</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="header" locale="fr">
                <value>Date d’expiration de la licence</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="header" locale="fr">
                <value>Numéro de licence</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="header" locale="fr">
                <value>Date d’inscription</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="header" locale="fr">
                <value>Numéro d’enregistrement</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="position">
          <configurationGroup name="fields">
            <configurationGroup name="health_facility">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Établissement de santé</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Établissement de santé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Titre du travail/poste</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="in">
                  <configuration name="header" locale="fr">
                    <value>Titre du travail/poste</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Titre du travail/poste</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="header" locale="fr">
                <value>Date de naissance</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Sexe</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Sexe</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_people" path="//modules/CustomReports/reportViews/search_people">
      <configuration name="description" locale="fr">
        <value>Rechercher des personnes dans le système.</value>
      </configuration>
      <configuration name="display_name" locale="fr">
        <value>Rechercher une personne</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>