<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainFacility" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="TrainFacility" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="fr">
        <value>Modifier la liste des établissements</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="fr">
        <value>Afficher la liste des établissements</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_owner" locale="fr">
        <value>Modifier la liste des agents d’établissement</value>
      </configuration>
      <configuration name="can_view_database_list_facility_owner" locale="fr">
        <value>Afficher la liste des agents d’établissement</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="fr">
        <value>Modifier la liste des types d’établissement</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="fr">
        <value>Afficher la liste des types d’établissement</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="fr">
            <value>Type d’établissement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_owner">
          <configuration name="display" values="single" locale="fr">
            <value>Propriété/Affiliation de l’établissement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="health_facility">
          <configuration name="display" values="single" locale="fr">
            <value>Établissement de santé</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_HealthFacility">
          <configurationGroup name="fields">
            <configurationGroup name="id_code">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Code d’identification</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" values="many" locale="fr">
                <value>default:Emplacement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility_owner">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Propriété/Affiliation de l’établissement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type d’établissement</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>