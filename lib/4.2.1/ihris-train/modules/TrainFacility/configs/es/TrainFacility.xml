<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainFacility" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="TrainFacility" path="/I2CE" locale="es">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="es">
        <value>Editar la lista de instalaciones</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="es">
        <value>Ver la lista de instalaciones</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_owner" locale="es">
        <value>Editar la lista de agente de instalaciones</value>
      </configuration>
      <configuration name="can_view_database_list_facility_owner" locale="es">
        <value>Ver la lista de agente de instalaciones</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="es">
        <value>Editar el tipo de lista de la institución</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="es">
        <value>Ver el tipo de lista de la institución</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipo de institución</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_owner">
          <configuration name="display" values="single" locale="es">
            <value>Propiedad/Afiliación de la institución</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="health_facility">
          <configuration name="display" values="single" locale="es">
            <value>Centro de salud</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_HealthFacility">
          <configurationGroup name="fields">
            <configurationGroup name="id_code">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Código de identificación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" values="many" locale="es">
                <value>default:Ubicación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility_owner">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Propiedad/Afiliación de la institución</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Tipo de institución</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>