<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonExam" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonExam" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_person_exam">
        <value>Peut voir le formulaire enfant person_exam d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_exam">
        <value>Peut modifier les formulaires enfant person_exam d’une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="exam_try">
        <configurationGroup name="first">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:1er essai</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="retry">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:2ème essai</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="final">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:Dernier essai</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="paper">
        <configurationGroup name="paper_i">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:Papier I</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="paper_ii">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:Papier II</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="paper_iii">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:Papier III</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="paper_iv">
          <configuration name="fields" values="many" type="delimited" locale="fr">
            <value>name:Papier IV</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="exam_grade">
        <configurationGroup name="grade_1">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_2">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_3">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_4">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_5">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_6">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_7">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="grade_8">
          <configuration name="fields" values="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="exam_try">
          <configuration name="display" values="single" locale="fr">
            <value>Passage d’examen</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="exam_grade">
          <configuration name="display" values="single" locale="fr">
            <value>Grade de l’examen</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="paper">
          <configuration name="display" values="single" locale="fr">
            <value>Papier</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_exam">
          <configuration name="display" values="single" locale="fr">
            <value>Examen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonExam">
          <configurationGroup name="fields">
            <configurationGroup name="exam_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de l'examen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="try">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Passage d’examen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="results">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Résultats de l’examen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="paper">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Papier</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="exam_grade">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Grade de l’examen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>