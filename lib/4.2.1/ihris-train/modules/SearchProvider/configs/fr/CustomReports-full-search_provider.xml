<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports-full-search_provider" path="/modules/CustomReports">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports-full-search_provider" path="/modules/CustomReports" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="search_provider" path="//modules/CustomReports/relationships/search_provider">
      <configuration name="display_name" locale="fr">
        <value>Rechercher un fournisseur de formation</value>
      </configuration>
      <configuration name="description" locale="fr">
        <value>rechercher une relation dans tous les enregistrements de fournisseur de formation</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="provider_training">
          <configuration name="description" locale="fr">
            <value>Affiche les formations affectées à un fournisseur</value>
          </configuration>
          <configuration name="display_name" locale="fr">
            <value>Fournisseur de formation</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="provider_instance">
              <configuration name="description" locale="fr">
                <value>Affiche les instances des formations</value>
              </configuration>
              <configuration name="display_name" locale="fr">
                <value>Instance de formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_training_provider" path="//modules/CustomReports/reports/search_training_provider">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="fr">
          <value>Rechercher un fournisseur de formation</value>
        </configuration>
        <configuration name="description" locale="fr">
          <value>Rechercher tous les enregistrements de fournisseur de formation</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="fr">
                    <value>Nom du fournisseur de formation</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Nom</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="trainingprovider_type">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Type de fournisseur de formation</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Type de fournisseur de formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="provider_training">
          <configurationGroup name="fields">
            <configurationGroup name="training">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="fr">
                    <value>Information de formation</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="fr">
                <value>Formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="provider_instance">
          <configurationGroup name="fields">
            <configurationGroup name="end_date">
              <configuration name="header" locale="fr">
                <value>Date de fin de formation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="header" locale="fr">
                <value>Détails de l’emplacement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="providertraining">
              <configuration name="header" locale="fr">
                <value>Instance de formation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="header" locale="fr">
                <value>Date de début de la formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="search_training_provider" path="//modules/CustomReports/reportViews/search_training_provider">
      <configuration name="description" locale="fr">
        <value>Rechercher tous les fournisseurs de formation dans le système</value>
      </configuration>
      <configuration name="display_name" locale="fr">
        <value>Rechercher un fournisseur de formation</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>