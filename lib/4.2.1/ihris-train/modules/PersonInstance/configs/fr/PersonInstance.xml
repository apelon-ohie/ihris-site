<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonInstance" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonInstance" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_person_instance">
        <value>Peut voir le formulaire enfant person_instance d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_instance">
        <value>Peut modifier les formulaires enfant person_instance d’une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="exam_result">
          <configuration name="display" values="single" locale="fr">
            <value>Résultat d’examen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonInstance">
          <configurationGroup name="fields">
            <configurationGroup name="attending">
              <configuration name="headers" locale="fr" type="delimited">
                <value>default:Est présent</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>