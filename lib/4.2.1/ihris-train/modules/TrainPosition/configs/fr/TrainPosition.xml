<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="TrainPosition" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="TrainPosition" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_position" locale="fr">
        <value>Peut voir le formulaire enfant de position d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_position" locale="fr">
        <value>Peut modifier les formulaires enfant de position d’une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour l’information de position</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="fr">
            <value>Information de position</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="health_facility">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Établissement de santé</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Titre du travail/poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_code">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Code du travail/poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de la nomination actuelle</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>