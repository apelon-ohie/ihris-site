<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="FeeType" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="FeeType" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_fee_type" locale="fr">
        <value>Modifier la liste des types de frais</value>
      </configuration>
      <configuration name="can_view_database_list_fee_type" locale="fr">
        <value>Afficher la liste des types de frais</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="unit">
        <configurationGroup name="total">
          <configuration name="fields" value="many" type="delimited" locale="fr"></configuration>
        </configurationGroup>
        <configurationGroup name="per_day">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Par jour</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="student_per_day">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Par jour et par étudiant</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facilitator_per_day">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Par animateur et par jour</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="student">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Par étudiant</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facilitator">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Par animateur</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="fee_type">
          <configuration name="display" values="single" locale="fr">
            <value>Information sur les frais</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="unit">
          <configuration name="display" values="single" locale="fr">
            <value>Unité</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_FeeType">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="unit">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Unité</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_length_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type de durée de formation</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>