<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ExamPaper" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ExamPaper" path="/I2CE" locale="fr">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_exampaper" locale="fr">
        <value>Modifier la liste des papiers d’examen</value>
      </configuration>
      <configuration name="can_view_database_list_exampaper" locale="fr">
        <value>Afficher la liste des fournisseurs de formation</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="exampaper">
          <configuration name="display" values="single" locale="fr">
            <value>Information sur les papiers d’examen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ExamPaper">
          <configurationGroup name="fields">
            <configurationGroup name="paper">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Papier</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Formation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="content">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Contenu</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>