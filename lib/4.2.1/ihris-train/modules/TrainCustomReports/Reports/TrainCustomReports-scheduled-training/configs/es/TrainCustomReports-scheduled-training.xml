<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-train-CustomReports-scheduled-training" path="/modules/CustomReports">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-train-CustomReports-scheduled-training" path="/modules/CustomReports" locale="es">
    <version>4.2.0.1</version>
    <configurationGroup name="scheduled_training" path="//modules/CustomReports/relationships/scheduled_training">
      <configuration name="description" locale="es">
        <value>Relación de todas las capacitaciones programadas.</value>
      </configuration>
      <configuration name="display_name" locale="es">
        <value>Capacitaciones programadas</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="funder">
          <configuration name="description" locale="es">
            <value>El financiador de esta capacitación.</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Financiador</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="providertraining">
          <configuration name="description" locale="es">
            <value>El enlace del proveedor a los detalles de la capacitación.</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Enlace de capacitación del proveedor</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="training">
              <configuration name="description" locale="es">
                <value>Los detalles de la capacitación.</value>
              </configuration>
              <configuration name="display_name" locale="es">
                <value>Capacitación</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="trainingprovider">
          <configuration name="description" locale="es">
            <value>El proveedor de capacitación para esta capacitación.</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Proveedor de capacitación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="reporting_functions">
        <configurationGroup name="completed">
          <configuration name="description" locale="es">
            <value>Establece a sí si esta capacitación se ha completado.</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="scheduled_training" path="//modules/CustomReports/reports/scheduled_training">
      <configurationGroup name="meta">
        <configuration name="description" locale="es">
          <value>Una lista de todas las capacitaciones programadas.</value>
        </configuration>
        <configuration name="display_name" locale="es">
          <value>Capacitaciones programadas</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="funder">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="es">
                <value>Financiador</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="end_date">
              <configuration name="header" locale="es">
                <value>Fecha de finalización</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="es">
                    <value>Fecha de finalización después</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="es">
                    <value>Fecha de finalización antes</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
            <configurationGroup name="funder">
              <configuration name="header" locale="es">
                <value>Id. de financiador</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Financiador</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="header" locale="es">
                <value>Fecha de inicio</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="es">
                    <value>Fecha de inicio después</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="greaterthan_equals_now">
                  <configuration name="header" locale="es">
                    <value>Fecha de inicio después de hoy</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="es">
                    <value>Fecha de inicio antes</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="providertraining">
          <configurationGroup name="fields">
            <configurationGroup name="training">
              <configuration name="header" locale="es">
                <value>Id. de capacitación</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="in">
                  <configuration name="header" locale="es">
                    <value>Capacitación</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="training">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="es">
                <value>Capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_classification">
              <configuration name="header" locale="es">
                <value>Clasificación de capacitación</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Clasificación de capacitación</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
            <configurationGroup name="training_length">
              <configuration name="header" locale="es">
                <value>Duración de la capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_length_type">
              <configuration name="header" locale="es">
                <value>Unidad de duración</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_type">
              <configuration name="header" locale="es">
                <value>Tipo de capacitación</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Tipo de capacitación</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="trainingprovider">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="es">
                <value>Proveedor de capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="trainingprovider_type">
              <configuration name="header" locale="es">
                <value>Tipo de proveedor de capacitación</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Tipo de proveedor de capacitación</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="reporting_functions">
        <configurationGroup name="completed">
          <configuration name="header" locale="es">
            <value>¿Completado?</value>
          </configuration>
          <configurationGroup name="limits">
            <configurationGroup name="yesno">
              <configuration name="header" locale="es">
                <value>¿Completado?</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="scheduled_training" path="//modules/CustomReports/reportViews/scheduled_training">
      <configuration name="description" locale="es">
        <value>Una lista de todas las capacitaciones programadas.</value>
      </configuration>
      <configuration name="display_name" locale="es">
        <value>Capacitaciones programadas</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>