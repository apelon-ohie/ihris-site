<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports" locale="uk">
    <version>4.2.0.1</version>
    <configurationGroup name="relationship-institution" path="relationships/institution">
      <configuration name="display_name" locale="uk">
        <value>Навчальний заклад</value>
      </configuration>
      <configuration name="description" locale="uk">
        <value>Зв'язок для всіх навчальних закладів в системі.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="facility_contact">
          <configuration name="description" locale="uk">
            <value>Контактні дані для цього закладу.</value>
          </configuration>
          <configuration name="display_name" locale="uk">
            <value>Контакти</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configuration name="description" locale="uk">
            <value>Вербувальний агент для цього закладу.</value>
          </configuration>
          <configuration name="display_name" locale="uk">
            <value>Агент закладу</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="description" locale="uk">
            <value>Район, де знаходиться цей заклад.</value>
          </configuration>
          <configuration name="display_name" locale="uk">
            <value>Район</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-institution" path="reports/institution">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="uk">
          <value>Звіти навчального закладу</value>
        </configuration>
        <configuration name="description" locale="uk">
          <value>Список всіх навчальних закладів.</value>
        </configuration>
        <configuration name="category" locale="uk">
          <value>Навчальні заклади</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="facility_agent">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="uk">
                    <value>Агент закладу</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>Ідентифікатор агента навчального закладу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="header" locale="uk">
                <value>Назва Навчального Закладу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="uk">
                    <value>Розташування навчального закладу</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>Ідентифікатор розташування навчального закладу</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="district">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="uk">
                <value>Район</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="uk">
                <value>Агент закладу</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="header" locale="uk">
                <value>Поштова адреса</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportView-institution" path="reportViews/institution">
      <configuration name="description" locale="uk">
        <value>Список всіх навчальних закладів.</value>
      </configuration>
      <configuration name="display_name" locale="uk">
        <value>Навчальні заклади</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>