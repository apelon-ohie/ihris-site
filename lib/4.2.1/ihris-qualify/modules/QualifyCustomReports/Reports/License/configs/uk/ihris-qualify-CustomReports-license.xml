<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-license" path="//modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-license" path="//modules/CustomReports" locale="uk">
    <version>4.2.0.1</version>
    <configurationGroup name="relationship-license" path="relationships/license">
      <configuration name="description" locale="uk">
        <value>Звіти, засновані на чинних ліцензіях у системі.</value>
      </configuration>
      <configuration name="display_name" locale="uk">
        <value>Ліцензія</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="training">
          <configuration name="description" locale="uk">
            <value>Навчання, пов'язане з цією ліцензією.</value>
          </configuration>
          <configuration name="display_name" locale="uk">
            <value>Навчання</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="cadre">
              <configuration name="description" locale="uk">
                <value>Кадри, пов'язані з цим навчанням.</value>
              </configuration>
              <configuration name="display_name" locale="uk">
                <value>Кадровий склад</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration">
              <configuration name="description" locale="uk">
                <value>Реєстраційні відомості для цього навчання.</value>
              </configuration>
              <configuration name="display_name" locale="uk">
                <value>Реєстрація</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="description" locale="uk">
                <value>Особа, яка пройшла це навчання.</value>
              </configuration>
              <configuration name="display_name" locale="uk">
                <value>Особа</value>
              </configuration>
              <configurationGroup name="joins">
                <configurationGroup name="demographic">
                  <configuration name="description" locale="uk">
                    <value>Демографічні відомості для цієї особи.</value>
                  </configuration>
                  <configuration name="display_name" locale="uk">
                    <value>Демографія</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-license" path="reports/license">
      <configurationGroup name="meta">
        <configuration name="category" locale="uk">
          <value>Звіти про навчання</value>
        </configuration>
        <configuration name="description" locale="uk">
          <value>Звіти, засновані на чинних ліцензіях у системі.</value>
        </configuration>
        <configuration name="display_name" locale="uk">
          <value>Звіт про ліцензію</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="cadre">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="uk">
                <value>Кадровий склад</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="person">
          <configurationGroup name="fields">
            <configurationGroup name="firstname">
              <configuration name="header" locale="uk">
                <value>Ім'я</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="surname">
              <configuration name="header" locale="uk">
                <value>Прізвище</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="end_date">
              <configuration name="header" locale="uk">
                <value>Дата закінчення ліцензії</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата закінчення ліцензії після</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата закінчення ліцензії перед</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="header" locale="uk">
                <value>Номер ліцензії</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="header" locale="uk">
                <value>Дата початку ліцензії</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата початку ліцензії після</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата початку ліцензії перед</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="training">
          <configurationGroup name="fields">
            <configurationGroup name="cadre">
              <configuration name="header" locale="uk">
                <value>ID Підготовки кадрів</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="uk">
                    <value>Кадровий склад</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
            <configurationGroup name="graduation">
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата закінчення після</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата закінчення перед</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>Дата випуску</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configurationGroup name="fields">
            <configurationGroup name="gender">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="uk">
                    <value>Стать</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>Стать</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportViews">
      <configurationGroup name="lisc_health_workers">
        <configuration name="description" locale="uk">
          <value>Список всіх ліцензованих медичних працівників.</value>
        </configuration>
        <configuration name="display_name" locale="uk">
          <value>Ліцензовані медичні працівники</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="lisc_by_cadre">
        <configuration name="description" locale="uk">
          <value>Діаграма всіх ліцензованих медичних працівників по кадрам.</value>
        </configuration>
        <configuration name="display_name" locale="uk">
          <value>Ліцензовані медичні працівники по кадрам</value>
        </configuration>
        <configurationGroup name="display_options">
          <configurationGroup name="PieChart">
            <configurationGroup name="styles">
              <configurationGroup name="Pie">
                <configuration name="display_name" locale="uk">
                  <value>Кругова діаграма (проста)</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="Bar">
                <configuration name="display_name" locale="uk">
                  <value>Гістограма (проста)</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="Column">
                <configuration name="display_name" locale="uk">
                  <value>Стовпчикова діаграма (проста)</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="StackedColumn">
                <configuration name="display_name" locale="uk">
                  <value>Гістограма з накопиченням (проста)</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="Scatter">
                <configuration name="display_name" locale="uk">
                  <value>Точкова діаграма</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>