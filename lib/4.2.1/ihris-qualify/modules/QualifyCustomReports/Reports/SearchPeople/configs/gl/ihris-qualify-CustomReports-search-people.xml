<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-search-people" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-search-people" path="/modules/CustomReports" locale="gl">
    <version>4.2.0.1</version>
    <configurationGroup name="search_people" path="relationships/search_people">
      <configuration name="display_name" locale="gl">
        <value>Procurar persoas</value>
      </configuration>
      <configuration name="description" locale="gl">
        <value>A relación usada para a páxina de procura de persoas.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="last_training">
          <configuration name="description" locale="gl">
            <value>A formación máis recente que tivo esta persoa.</value>
          </configuration>
          <configuration name="display_name" locale="gl">
            <value>Última formación</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="cadre">
              <configuration name="description" locale="gl">
                <value>O cadro da formación máis recente.</value>
              </configuration>
              <configuration name="display_name" locale="gl">
                <value>Cadro</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configuration name="description" locale="gl">
            <value>Detalles demográficos desta persoa.</value>
          </configuration>
          <configuration name="display_name" locale="gl">
            <value>Datos demográficos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="SearchPeopleReport" path="reports/search_people">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="gl">
          <value>Procurar persoas</value>
        </configuration>
        <configuration name="description" locale="gl">
          <value>Procurar calquera persoa no sistema coa formación máis recente.</value>
        </configuration>
        <configuration name="category" locale="gl">
          <value>Procurar informes</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="surname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="gl">
                    <value>Apelidos</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="gl">
                <value>Apelidos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="firstname">
              <configuration name="header" locale="gl">
                <value>Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="header" locale="gl">
                <value>Outros nomes</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="last_training">
          <configurationGroup name="fields">
            <configurationGroup name="index_num">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="gl">
                    <value>Número de índice</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="gl">
                <value>Número de índice</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="gl">
                    <value>Cadro</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="gl">
                <value>ID do cadro de formación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="intake_date">
              <configuration name="header" locale="gl">
                <value>Data de admisión</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="cadre">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="gl">
                <value>Cadro</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="SearchPeopleView" path="reportViews/search_people">
      <configuration name="description" locale="gl">
        <value>Procurar persoas no sistema.</value>
      </configuration>
      <configuration name="display_name" locale="gl">
        <value>Procurar persoas</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>