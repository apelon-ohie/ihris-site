<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-registration" path="//modules/CustomReports">
  <metadata>
    <version>4.2.2.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-registration" path="//modules/CustomReports" locale="uk">
    <version>4.2.0.1</version>
    <configurationGroup name="relationship-registration" path="relationships/registration">
      <configuration name="display_name" locale="uk">
        <value>Реєстрація</value>
      </configuration>
      <configuration name="description" locale="uk">
        <value>Зв'язок для звітів, які займаються реєстрацією.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="training">
          <configuration name="description" locale="uk">
            <value>Навчання, пов'язане з цією реєстрацією.</value>
          </configuration>
          <configuration name="display_name" locale="uk">
            <value>Навчання</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="cadre">
              <configuration name="description" locale="uk">
                <value>Кадри, пов'язані з цим навчанням.</value>
              </configuration>
              <configuration name="display_name" locale="uk">
                <value>Кадровий склад</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="person">
              <configuration name="description" locale="uk">
                <value>Особа, яка пройшла це навчання.</value>
              </configuration>
              <configuration name="display_name" locale="uk">
                <value>Особа</value>
              </configuration>
              <configurationGroup name="joins">
                <configurationGroup name="personal_contact">
                  <configuration name="description" locale="uk">
                    <value>Персональна контактна інформація для цієї людини.</value>
                  </configuration>
                  <configuration name="display_name" locale="uk">
                    <value>Особистий контакт</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-registration" path="reports/registration">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="uk">
          <value>Реєстраційний звіт</value>
        </configuration>
        <configuration name="description" locale="uk">
          <value>Звіт, заснований на реєстраціях у системі.</value>
        </configuration>
        <configuration name="category" locale="uk">
          <value>Підготовка звітів</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="registration_number">
              <configuration name="header" locale="uk">
                <value>Реєстраційний номер</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="header" locale="uk">
                <value>Дата реєстрації</value>
              </configuration>
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата реєстрації після</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата реєстрації перед</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="training">
          <configurationGroup name="fields">
            <configurationGroup name="cadre">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="uk">
                    <value>Кадровий склад</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>ID Підготовки кадрів</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="intake_date">
              <configurationGroup name="limits">
                <configurationGroup name="greaterthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата прийому після</value>
                  </configuration>
                </configurationGroup>
                <configurationGroup name="lessthan_equals">
                  <configuration name="header" locale="uk">
                    <value>Дата прийому перед</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="uk">
                <value>Дата прийому</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="person">
          <configurationGroup name="fields">
            <configurationGroup name="surname">
              <configuration name="header" locale="uk">
                <value>Прізвище</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="firstname">
              <configuration name="header" locale="uk">
                <value>Ім'я</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="personal_contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="header" locale="uk">
                <value>Поштова адреса</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="cadre">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="uk">
                <value>Кадровий склад</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportView-registration" path="reportViews/registration">
      <configuration name="description" locale="uk">
        <value>Список всіх зареєстрованих медичних працівників.</value>
      </configuration>
      <configuration name="display_name" locale="uk">
        <value>Зареєстровані медичні працівники</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>